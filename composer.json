{
  "name": "skiphog/rest",
  "license": "MIT",
  "description": "Test Application rest",
  "require": {
    "php": "^7.1",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "nilportugues/sql-query-builder": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "System\\": "system/"
    },
    "files": [
      "system/Helpers/functions.php"
    ]
  },
  "autoload-dev": {},
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
